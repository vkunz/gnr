<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="GNRMainFrame">
		<title>GNR - 3D Einrichtungsplaner</title>
		<pos>0,0</pos>
		<size>800,550</size>
		<focused>1</focused>
		<minsize>800,480</minsize>
		<id_arg>0</id_arg>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;Datei</label>
				<object class="wxMenuItem" name="idMenuNewRoom" variable="MenuItem8" member="no">
					<label>&amp;Neuer Raum</label>
					<accel>Alt-N</accel>
					<help>Raum leeren...</help>
					<handler function="OnMenuNewRoom" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="idMenuOpxOpen" variable="MenuItem3" member="no">
					<label>OPX &amp;Öffnen</label>
					<accel>Alt-O</accel>
					<help>vorhandene Datei öffnen...</help>
					<handler function="OnMenuOpxOpen" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuOpxSave" variable="MenuItem4" member="no">
					<label>OPX &amp;Speichern</label>
					<accel>Alt-S</accel>
					<help>Datei speichern...</help>
					<handler function="OnMenuOpxSave" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="idMenuOaxImport" variable="MenuItem10" member="yes">
					<label>OAX Importieren</label>
					<help>OAX Importieren...</help>
					<handler function="OnMenuOaxImport" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuOaxExport" variable="MenuItem11" member="yes">
					<label>OAX Exportieren</label>
					<help>OAX Exportieren...</help>
					<handler function="OnMenuOaxExport" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="idMenuObjImport" variable="MenuItem6" member="no">
					<label>OBJ &amp;Importieren</label>
					<accel>Alt-I</accel>
					<help>Object-Datei importieren...</help>
					<handler function="OnMenuObjImport" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuObjExport" variable="MenuItem7" member="no">
					<label>OBJ E&amp;xportieren</label>
					<accel>Alt-X</accel>
					<help>Object-Datei exportieren...</help>
					<handler function="OnMenuObjExport" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>GNR Schließen</label>
					<accel>Alt-F4</accel>
					<help>GNR beenden...</help>
					<handler function="OnMenuQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>&amp;Einstellungen</label>
				<object class="wxMenuItem" name="idMenuSnapToGrid" variable="MenuItem9" member="yes">
					<label>Ein&amp;rasten aktivieren</label>
					<accel>ALT-R</accel>
					<help>Einrasten aktivieren</help>
					<check>1</check>
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="idMenuMoveXZ" variable="MenuItem12" member="yes">
					<label>Verschieben XZ</label>
					<accel>ALT-1</accel>
					<help>Objekt verschieben entlang X- und Z-Achse...</help>
					<radio>1</radio>
				</object>
				<object class="wxMenuItem" name="idMenuMoveXY" variable="MenuItem13" member="yes">
					<label>Verschieben XY</label>
					<accel>ALT-2</accel>
					<help>Objekt verschieben entlang X- und Y-Achse...</help>
					<radio>1</radio>
				</object>
				<object class="wxMenuItem" name="idMenuRotateXZ" variable="MenuItem14" member="yes">
					<label>Rotieren XZ</label>
					<accel>ALT-3</accel>
					<help>Objekt rotieren an X- und Z-Achse...</help>
					<radio>1</radio>
				</object>
				<object class="wxMenuItem" name="idMenuRotateXY" variable="MenuItem15" member="yes">
					<label>Rotieren XY</label>
					<accel>ALT-4</accel>
					<help>Objekt rotieren an X- und Y-Achse...</help>
					<radio>1</radio>
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>&amp;Hilfe</label>
				<object class="wxMenuItem" name="idMenuHelp" variable="MenuItem5" member="no">
					<label>&amp;Hilfe</label>
					<accel>F1</accel>
					<help>Hilfe zur Anwendung</help>
				</object>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>&amp;Über</label>
					<accel>Alt-F1</accel>
					<help>Informationen über GNR...</help>
					<handler function="OnMenuAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_StatusBar" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
