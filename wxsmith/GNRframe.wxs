<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="GNRFrame">
		<size>690,311</size>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;Datei</label>
				<object class="wxMenuItem" name="idMenuLoad" variable="MenuItem3" member="yes">
					<label>&amp;Öffnen</label>
					<accel>Alt-O</accel>
					<help>vorhandene Datei öffnen...</help>
				</object>
				<object class="wxMenuItem" name="idMenuSave" variable="MenuItem4" member="no">
					<label>&amp;Speichern</label>
					<accel>Alt-S</accel>
					<help>Datei speichern...</help>
				</object>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>&amp;Schließen</label>
					<accel>Alt-F4</accel>
					<help>GNR schließen...</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>&amp;Hilfe</label>
				<object class="wxMenuItem" name="idMenuHelp" variable="MenuItem5" member="yes">
					<label>&amp;Hilfe</label>
					<accel>F1</accel>
					<help>Hilfe zur Anwendung</help>
				</object>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>&amp;Über</label>
					<accel>Alt-F1</accel>
					<help>Informationen über GNR...</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
